{
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": [
        "--skip-string-normalization",
        "--line-length",
        "120"
    ],
    "python.linting.pylintEnabled": false,
    "python.linting.enabled": true,
    "peacock.remoteColor": "#620fb5",
    "extensions": {
        "recommendations": [
            "thebarkman.vscode-djaneiro",
        ]
    },
    "python.linting.flake8Enabled": true,
    "python.testing.pytestArgs": [
        "app"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#0a667f",
        "activityBar.background": "#0a667f",
        "activityBar.foreground": "#e7e7e7",
        "activityBar.inactiveForeground": "#e7e7e799",
        "activityBarBadge.background": "#590747",
        "activityBarBadge.foreground": "#e7e7e7",
        "commandCenter.border": "#e7e7e799",
        "sash.hoverBorder": "#0a667f",
        "statusBar.background": "#064050",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#0a667f",
        "statusBarItem.remoteBackground": "#064050",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.activeBackground": "#064050",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#06405099",
        "titleBar.inactiveForeground": "#e7e7e799"
    },
    "peacock.color": "#064050"

}
