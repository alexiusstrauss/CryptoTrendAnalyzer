{
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": [
        "--skip-string-normalization",
        "--line-length",
        "120"
    ],
    "python.linting.pylintEnabled": false,
    "python.linting.enabled": true,
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#7b13e4",
        "activityBar.background": "#7b13e4",
        "activityBar.foreground": "#e7e7e7",
        "activityBar.inactiveForeground": "#e7e7e799",
        "activityBarBadge.background": "#3b2005",
        "activityBarBadge.foreground": "#e7e7e7",
        "commandCenter.border": "#e7e7e799",
        "sash.hoverBorder": "#7b13e4",
        "statusBar.background": "#620fb5",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#7b13e4",
        "statusBarItem.remoteBackground": "#620fb5",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.activeBackground": "#620fb5",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#620fb599",
        "titleBar.inactiveForeground": "#e7e7e799"
    },
    "peacock.remoteColor": "#620fb5",
    "extensions": {
        "recommendations": [
            "thebarkman.vscode-djaneiro",
        ]
    },
    "python.linting.flake8Enabled": true,
    "python.testing.pytestArgs": [
        "app"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true

}
